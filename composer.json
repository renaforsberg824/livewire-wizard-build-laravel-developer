{
    "name": "spatie/laravel-livewire-wizard",
    "description": "Build wizards using Livewire",
    "keywords": [
        "spatie",
        "laravel",
        "laravel-livewire-wizard"
    ],
    "homepage": "https://github.com/spatie/laravel-livewire-wizard",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "role": "Developer"
        },
        {
            "name": "Rias Van der Veken",
            "email": "rias@spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^9.0",
        "livewire/livewire": "^2.10",
        "spatie/laravel-collection-macros": "^7.11",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "require-dev": {
        "ext-dom": "*",
        "nunomaduro/collision": "^6.0",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26",
        "spatie/pest-plugin-snapshots": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\LivewireWizard\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\LivewireWizard\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\LivewireWizard\\WizardServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
